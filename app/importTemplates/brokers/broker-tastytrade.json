            "Quantity": {
              "prop": "qty"
            },
            "Price": {
              "prop": "price"
            },
            "Fees": {
              "prop": "fee"
            },
            "$feeCurrency": {
              "prop": "feeCurrency",
              "mapping": {
                "type": "value",
                "value": {
                  "id": "c-USD"
                }
              }
            },
            "$valueCurrency": {
              "prop": "valueCurrency",
              "mapping": {
                "type": "value",
                "value": {
