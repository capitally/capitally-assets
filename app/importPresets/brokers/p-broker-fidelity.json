{
  "version": 3,
  "type": "Capitally Data Export",
  "exportedAt": "2024-12-04T21:25:19.525Z",
  "data": {
    "importPresets": {
      "u-4SgeUlfcnj2vo2h": {
        "id": "u-4SgeUlfcnj2vo2h",
        "name": "Transactions & dividends",
        "updatedAt": "2024-12-04T22:25",
        "author": "RL <iFIjgKg2M7TIe5ygfjFzaDEAlAX2>",
        "info": "To import your transactions from Fidelity:\n\n1. Login to your Fidelity Client Portal\n2. Choose **Account** ‚Üí **Activity & Orders**\n3. Choose a time period\n4. Download transactions in CSV format with the green ‚Üì icon\n5. Open or drop that file here",
        "sourceId": "p-broker-fidelity",
        "provider": "Fidelity",
        "providerIcon": {
          "type": "logo",
          "id": "brokers/fidelity"
        },
        "providerGroup": "Brokers",
        "parser": {
          "dateOrder": "mdy",
          "decimalSeparator": ".",
          "allowClipboard": false,
          "csv": {}
        },
        "sections": {
          "main": {
            "csv": {
              "fromRow": "~Run Date~^",
              "untilRow": "/[a-z ]{5,}/i^",
              "header": true
            },
            "replaceHeaders": [
              ["~( )~", ""],
              ["~Security Description~^$", "Description"],
              ["~Security Type~^$", "Type"]
            ],
            "reverseRows": true,
            "enabledColumns": [
              "Run Date",
              "Action",
              "Symbol",
              "Description",
              "Type",
              "Quantity",
              "Price",
              "Commission",
              "Fees",
              "Accrued Interest",
              "Amount",
              "Cash Balance",
              "Settlement Date"
            ],
            "optionalColumns": ["Cash Balance"],
            "program": {
              "stmts": [
                {
                  "type": "addAccount",
                  "optional": true,
                  "stmts": [
                    {
                      "type": "set",
                      "prop": "account.name",
                      "xpr": {
                        "type": "value",
                        "value": "Fidelity"
                      }
                    },
                    {
                      "type": "set",
                      "prop": "account.icon",
                      "xpr": {
                        "type": "value",
                        "value": {
                          "type": "logo",
                          "id": "brokers/fidelity"
                        }
                      }
                    },
                    {
                      "type": "set",
                      "prop": "account.countryISO",
                      "xpr": {
                        "type": "value",
                        "value": "US"
                      }
                    }
                  ]
                },
                {
                  "type": "if",
                  "ifs": [
                    {
                      "xpr": {
                        "type": "pattern",
                        "xpr": {
                          "type": "prop",
                          "prop": "cols.Description"
                        },
                        "pattern": "~UNITED STATES TREAS ~^"
                      },
                      "stmts": [
                        {
                          "type": "addRowNote",
                          "note": {
                            "type": "value",
                            "value": "üè¶ Treasury bills and notes are currently not imported"
                          }
                        },
                        {
                          "type": "return"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "addTransaction",
                  "stmts": [
                    {
                      "type": "set",
                      "prop": "transaction.account",
                      "xpr": {
                        "type": "prop",
                        "prop": "addedAccount"
                      }
                    },
                    {
                      "type": "set",
                      "prop": "transaction.date",
                      "xpr": {
                        "type": "prop",
                        "prop": "cols.Run Date"
                      }
                    },
                    {
                      "type": "set",
                      "prop": "transaction.type",
                      "xpr": {
                        "type": "prop",
                        "prop": "cols.Action"
                      },
                      "trfms": [
                        {
                          "type": "trim"
                        }
                      ],
                      "map": [
                        {
                          "cmp": {
                            "type": "pattern",
                            "pattern": "~YOU BOUGHT~^"
                          },
                          "target": {
                            "type": "value",
                            "value": "OPEN"
                          }
                        },
                        {
                          "cmp": {
                            "type": "pattern",
                            "pattern": "~YOU SOLD~^"
                          },
                          "target": {
                            "type": "value",
                            "value": "CLOSE"
                          }
                        },
                        {
                          "cmp": {
                            "type": "pattern",
                            "pattern": "~DIVIDEND RECEIVED~^"
                          },
                          "target": {
                            "type": "value",
                            "value": "DIVIDEND"
                          }
                        },
                        {
                          "cmp": {
                            "type": "pattern",
                            "pattern": "~RECEIVED FROM YOU ~^"
                          },
                          "target": {
                            "type": "value",
                            "value": "DIVIDEND"
                          }
                        },
                        {
                          "target": {
                            "type": "value"
                          }
                        }
                      ]
                    },
                    {
                      "type": "if",
                      "ifs": [
                        {
                          "xpr": {
                            "type": "in",
                            "xpr": {
                              "type": "prop",
                              "prop": "transaction.type"
                            },
                            "v": {
                              "type": "value",
                              "value": ["OPEN", "CLOSE"]
                            }
                          },
                          "stmts": [
                            {
                              "type": "comment",
                              "md": "## Buy & Sell transactions"
                            },
                            {
                              "type": "set",
                              "prop": "transaction.qty",
                              "xpr": {
                                "type": "prop",
                                "prop": "cols.Quantity"
                              },
                              "trfms": [
                                {
                                  "type": "abs"
                                }
                              ]
                            },
                            {
                              "type": "set",
                              "prop": "transaction.price",
                              "xpr": {
                                "type": "prop",
                                "prop": "cols.Price"
                              }
                            }
                          ]
                        },
                        {
                          "xpr": {
                            "type": "in",
                            "xpr": {
                              "type": "prop",
                              "prop": "transaction.type"
                            },
                            "v": {
                              "type": "value",
                              "value": ["DIVIDEND"]
                            }
                          },
                          "stmts": [
                            {
                              "type": "comment",
                              "md": "## Dividends"
                            },
                            {
                              "type": "set",
                              "prop": "transaction.value",
                              "xpr": {
                                "type": "prop",
                                "prop": "cols.Amount"
                              }
                            },
                            {
                              "type": "comment",
                              "md": "Look for dividend reinvestment that should be added to this transaction"
                            },
                            {
                              "type": "lookupRows",
                              "direction": "after",
                              "offset": {
                                "type": "value",
                                "value": -5
                              },
                              "limit": {
                                "type": "value",
                                "value": 10
                              },
                              "stmts": [
                                {
                                  "type": "if",
                                  "ifs": [
                                    {
                                      "xpr": {
                                        "type": "and",
                                        "xprs": [
                                          {
                                            "type": "pattern",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Action"
                                            },
                                            "pattern": "~REINVESTMENT ~"
                                          },
                                          {
                                            "type": "=",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Symbol"
                                            },
                                            "v": {
                                              "type": "prop",
                                              "prop": "cols.Symbol"
                                            }
                                          },
                                          {
                                            "type": "=",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Run Date"
                                            },
                                            "v": {
                                              "type": "prop",
                                              "prop": "cols.Run Date"
                                            }
                                          }
                                        ]
                                      },
                                      "stmts": [
                                        {
                                          "type": "set",
                                          "prop": "transaction.incomeQty",
                                          "xpr": {
                                            "type": "prop",
                                            "prop": "lookup.cols.Quantity"
                                          }
                                        },
                                        {
                                          "type": "ignoreRow"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "if",
                                  "ifs": [
                                    {
                                      "xpr": {
                                        "type": "and",
                                        "xprs": [
                                          {
                                            "type": "pattern",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Action"
                                            },
                                            "pattern": "~FOREIGN TAX PAID ~"
                                          },
                                          {
                                            "type": "=",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Symbol"
                                            },
                                            "v": {
                                              "type": "prop",
                                              "prop": "cols.Symbol"
                                            }
                                          },
                                          {
                                            "type": "=",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Run Date"
                                            },
                                            "v": {
                                              "type": "prop",
                                              "prop": "cols.Run Date"
                                            }
                                          }
                                        ]
                                      },
                                      "stmts": [
                                        {
                                          "type": "set",
                                          "prop": "transaction.taxPaid",
                                          "xpr": {
                                            "type": "prop",
                                            "prop": "lookup.cols.Amount"
                                          },
                                          "trfms": [
                                            {
                                              "type": "negate"
                                            }
                                          ]
                                        },
                                        {
                                          "type": "ignoreRow"
                                        }
                                      ]
                                    },
                                    {
                                      "xpr": {
                                        "type": "and",
                                        "xprs": [
                                          {
                                            "type": "=",
                                            "xpr": {
                                              "type": "prop",
                                              "prop": "lookup.cols.Action"
                                            },
                                            "v": {
                                              "type": "value"
                                            }
                                          }
                                        ]
                                      },
                                      "stmts": []
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "xpr": {
                            "type": "or",
                            "xprs": [
                              {
                                "type": "pattern",
                                "xpr": {
                                  "type": "prop",
                                  "prop": "cols.Action"
                                },
                                "pattern": "~INTEREST EARNED~"
                              }
                            ]
                          },
                          "stmts": [
                            {
                              "type": "comment",
                              "md": "## Interest"
                            },
                            {
                              "type": "addRowNote",
                              "note": {
                                "type": "value",
                                "value": "üè¶ Bonds and deposits are currently not imported"
                              }
                            },
                            {
                              "type": "return"
                            }
                          ]
                        },
                        {
                          "xpr": {
                            "type": "or",
                            "xprs": [
                              {
                                "type": "pattern",
                                "xpr": {
                                  "type": "prop",
                                  "prop": "cols.Action"
                                },
                                "pattern": "~CASH~"
                              },
                              {
                                "type": "pattern",
                                "xpr": {
                                  "type": "prop",
                                  "prop": "cols.Action"
                                },
                                "pattern": "~Electronic Funds Transfer~"
                              }
                            ]
                          },
                          "stmts": [
                            {
                              "type": "comment",
                              "md": "## Money transfers"
                            },
                            {
                              "type": "addRowNote",
                              "note": {
                                "type": "value",
                                "value": "üí∏ Money transfers are currently not imported"
                              }
                            },
                            {
                              "type": "return"
                            }
                          ]
                        },
                        {
                          "xpr": {
                            "type": "pattern",
                            "xpr": {
                              "type": "prop",
                              "prop": "cols.Action"
                            },
                            "pattern": "~REINVESTMENT~"
                          },
                          "stmts": [
                            {
                              "type": "comment",
                              "md": "Silently ignore, it's handled in the lookups"
                            },
                            {
                              "type": "return"
                            }
                          ]
                        },
                        {
                          "xpr": {
                            "type": "in",
                            "xpr": {
                              "type": "prop",
                              "prop": "transaction.type"
                            },
                            "v": {
                              "type": "value"
                            }
                          },
                          "stmts": [
                            {
                              "type": "addRowNote",
                              "note": {
                                "type": "formula",
                                "formula": "=\"ü•∫ Transaction `\" + col(\"Action\") + \"` is currently not supported\""
                              },
                              "urgency": "warn"
                            },
                            {
                              "type": "return"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "set",
                      "prop": "transaction.asset",
                      "xpr": {
                        "type": "prop",
                        "prop": "cols.Symbol"
                      },
                      "cast": {
                        "asset": {
                          "assetTypes": {
                            "type": "value",
                            "value": ["STOCK", "ETF", "FUND"]
                          },
                          "markets": {
                            "type": "value",
                            "value": [
                              {
                                "id": "mic-XNAS"
                              },
                              {
                                "id": "mic-XASE"
                              },
                              {
                                "id": "mic-BATS"
                              },
                              {
                                "id": "mic-XNYS"
                              },
                              {
                                "id": "mic-ARCX"
                              },
                              {
                                "id": "m-OTC"
                              },
                              {
                                "id": "m-USFUND"
                              }
                            ]
                          },
                          "remote": {
                            "type": "value",
                            "value": true
                          }
                        }
                      }
                    },
                    {
                      "type": "set",
                      "prop": "transaction.fee",
                      "xpr": {
                        "type": "prop",
                        "prop": "cols.Fees"
                      }
                    },
                    {
                      "type": "set",
                      "prop": "transaction.currency",
                      "xpr": {
                        "type": "value",
                        "value": {
                          "id": "c-USD"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          }
        },
        "options": {
          "valueMap": {
            "asset": {}
          }
        }
      }
    }
  }
}
