                            "v": {
                              "type": "value",
                              "value": "dyspozycja zakupu"
                            }
                          },
                          {
                            "type": "=",
                            "xpr": {
                              "type": "prop",
                              "prop": "cols.RODZAJ DYSPOZYCJI"
                            },
                            "v": {
                              "type": "value",
                              "value": "dyspozycja przedterminowego wykupu"
                            }
                          }
                        ]
                      },
                      "stmts": [
                        {
                          "type": "comment",
                          "md": "Silently ignore"
                        },
                        {
                          "type": "addRowNote",
                          "note": {
                            "type": "value",
                            "value": "Ignored"
                          },
                          "urgency": "log"
                        },
                        {
                          "type": "return"
                        }
                      ]
                    },
                    {
                      "stmts": [
                        {
                          "type": "addRowNote",
                          "note": {
                            "type": "formula",
                            "formula": "=\"Unknown transaction `\" + col(\"RODZAJ DYSPOZYCJI\") + \"`\""
                          },
                          "urgency": "warn"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          }
        }
