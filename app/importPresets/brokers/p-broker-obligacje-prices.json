        "author": "RL <iFIjgKg2M7TIe5ygfjFzaDEAlAX2>",
        "info": "â˜ï¸ This will update prices and thus value of bonds you've previously imported using **Obligacje Skarbowe: 1 of 2 - Transactions**. You'll need to run it regularly, as Capitally does not automatically update value of bonds.\n\nTo update the bond prices:\n\n1. Go to [Serwis Obligacji Skarbowych](https://www.zakup.obligacjeskarbowe.pl/login.html)\n2. Open **Rachunek Rejestrowy**\n3. Open **Stan Rachunku Rejestrowego**\n4. Click the **[â†“]** button to download the XLS file\n5. Open or drop that file here\n\n> ðŸ“‹ Alternatively, you can copy the *whole* table, including the headers, and paste it here.",
        "sourceId": "p-broker-obligacje-prices",
        "provider": "Obligacje Skarbowe",
        "providerIcon": {
          "type": "logo",
          "id": "brokers/ministerstwo-finansow"
        },
        "providerGroup": "Brokers",
        "parser": {
          "formats": [
            "xls"
          ],
          "allowClipboard": true,
          "csv": {}
        },
        "sections": {
          "main": {
            "csv": {
              "header": true
            },
            "replaceHeaders": [
              [
                "~Emisja~^$",
                "EMISJA"
              ],
              [
                "~Liczba obligacji~^$",
                "DOSTÄ˜PNA LICZBA OBLIGACJI"
              ],
              [
                "~WartoÅ›Ä‡ obligacji~^$",
                "ZABLOKOWANA LICZBA OBLIGACJI"
              ],
              [
                "~Data Wykupu~^$",
                "WARTOÅšÄ† NOMINALNA"
              ],
              [
                "~Column 5~^$",
                "WARTOÅšÄ† AKTUALNA"
              ],
              [
                "~Column 6~^$",
                "DATA WYKUPU"
              ]
            ],
            "enabledColumns": [
              "EMISJA",
              "DOSTÄ˜PNA LICZBA OBLIGACJI",
              "ZABLOKOWANA LICZBA OBLIGACJI",
              "WARTOÅšÄ† NOMINALNA",
              "WARTOÅšÄ† AKTUALNA",
              "DATA WYKUPU"
            ],
            "program": {
              "stmts": [
                {
                  "type": "if",
                  "ifs": [
                    {
                      "xpr": {
                        "type": "pattern",
                        "xpr": {
                          "type": "prop",
                          "prop": "cols.EMISJA"
                        },
                        "pattern": "/[A-Z]{3,5}[0-9]{4}/^$"
                      },
                      "stmts": [
                        {
                          "type": "var",
                          "varId": "principal",
                          "varType": "number",
                          "multiple": false,
                          "xpr": {
                            "type": "value",
                            "value": 0
                          }
                        },
                        {
                          "type": "var",
                          "varId": "value",
                          "varType": "number",
                          "multiple": false,
                          "xpr": {
                            "type": "value",
                            "value": 0
                          }
                        },
                        {
                          "type": "comment",
                          "md": "Collect bond values from all series"
                        },
                        {
                          "type": "lookupRows",
                          "direction": "after",
                          "stmts": [
                            {
                              "type": "if",
                              "ifs": [
                                {
                                  "xpr": {
                                    "type": "=",
                                    "xpr": {
                                      "type": "prop",
                                      "prop": "lookup.cols.EMISJA"
                                    },
                                    "v": {
                                      "type": "prop",
                                      "prop": "cols.EMISJA"
                                    }
                                  },
                                  "stmts": [
                                    {
                                      "type": "set",
                                      "prop": "$principal",
                                      "op": "+=",
                                      "xpr": {
                                        "type": "prop",
                                        "prop": "lookup.cols.WARTOÅšÄ† NOMINALNA"
                                      }
                                    },
                                    {
                                      "type": "set",
                                      "prop": "$value",
                                      "op": "+=",
                                      "xpr": {
                                        "type": "prop",
                                        "prop": "lookup.cols.WARTOÅšÄ† AKTUALNA"
                                      }
                                    },
                                    {
                                      "type": "ignoreRow"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "setMarketPrice",
                          "asset": {
                            "type": "prop",
                            "prop": "cols.EMISJA"
                          },
                          "date": {
                            "type": "formula",
                            "formula": "=D.today()"
                          },
                          "price": {
                            "type": "formula",
                            "formula": "=100 * $value / $principal"
                          }
                        }
                      ]
                    },
                    {
                      "stmts": [
                        {
                          "type": "addRowNote",
                          "note": {
                            "type": "value",
                            "value": "Ignored as a copy&paste artifact"
                          },
                          "urgency": "log"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          }
        }
      }
    }
  }
