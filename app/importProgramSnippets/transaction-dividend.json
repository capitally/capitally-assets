{
  "id": "import-transaction-dividend",
  "name": "Add Transaction: Dividend",
  "stmts": [
    {
      "type": "addTransaction",
      "stmts": [
        {
          "type": "set",
          "prop": "transaction.account",
          "xpr": { "type": "value" }
        },
        {
          "type": "set",
          "prop": "transaction.asset",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          },
          "cast": { "asset": { "remote": { "type": "value", "value": true } } }
        },
        {
          "type": "set",
          "prop": "transaction.type",
          "xpr": { "type": "value", "value": "DIVIDEND" },
          "map": []
        },
        {
          "type": "set",
          "prop": "transaction.date",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          }
        },
        {
          "type": "set",
          "prop": "transaction.value",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          }
        },
        {
          "type": "set",
          "prop": "transaction.valueCurrency",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          }
        },
        {
          "type": "set",
          "prop": "transaction.fee",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          },
          "trfms": [{ "type": "abs" }]
        },
        {
          "type": "set",
          "prop": "transaction.feeCurrency",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          }
        },
        {
          "type": "set",
          "prop": "transaction.taxPaid",
          "xpr": {
            "type": "prop",
            "prop": "cols.pick a property or enter value"
          },
          "trfms": [{ "type": "abs" }]
        }
      ]
    }
  ]
}
